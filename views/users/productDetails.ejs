        <section class="breadcrumb-option">
            <div class="container">
                <!-- Your breadcrumb content -->
                <div class="breadcrumb__text">
                    <h4>Shop - <%= categoryType %> - <%= categoryName %>
                    </h4>
                    <!-- Breadcrumb links -->
                    <div class="breadcrumb__links">
                        <a href="/home">Home</a>
                        <span>Shop - <%= categoryType %> - <%= categoryName %> - productDetails</span>
                    </div>
                </div>
            </div>
        </section>
        <section>
            <div class="row mt-5">
                <% if (products && products.length > 0) { %>
                    <% products.forEach((product, i) => { %>
                        <div class="col-lg-2 col-md-3 d-flex align-items-md-center justify-content-center">
                            <ul class="nav nav-tabs" role="tablist">
                                <% const productImages = product.images || []; %>
                                <% productImages.forEach((image, j) => { %>
                                    <li class="nav-item">
                                        <a class="nav-link <%= j === 0 ? 'active' : '' %>" data-toggle="tab" href="#tabs-<%= j + 1 %>" role="tab">
                                            <img src="/uploads/<%= image || 'no-image.webp' %>" style="background-size: cover; background-repeat: no-repeat; height: 120px; width: 95px;" alt="Product Image <%= j + 1 %>">
                                        </a>
                                    </li>
                                <% }) %>
                            </ul>
                        </div>
                        <div class="col-lg-8 col-md-9 d-flex align-items-left justify-content-left">
                            <div class="tab-content">
                                <% product.images.forEach((image, k) => { %>
                                    <div class="tab-pane <%= k === 0 ? 'active' : '' %>" id="tabs-<%= k + 1 %>" role="tabpanel">
                                        <!-- Display the product image and details in the same row -->
                                        <div class="row">
                                            <div class="col-lg-6 col-md-6">
                                                <!-- Display the product image -->
                                                <div class="product-image-container">
                                                    <img class="zoom" src="/uploads/<%= image || 'no-image.webp' %>" style="height: 600px;" alt="Product Image <%= k + 1 %>">
                                                </div>
                                            </div>
                                            <div class="col-lg-6 col-md-6">
                                                <!-- Render product details -->
                                                <h3><strong><%= product.name %></strong></h3>
                                                <div class="rating my-4">
                                                    <% const rating = 4; /* Replace this with your product's actual rating */ %>
                                                    <% for (let i = 0; i < 5; i++) { %>
                                                        <i class="fa fa-star<%= i < rating ? '' : '-o' %>"></i>
                                                    <% } %>
                                                    <!-- Display the rating value -->
                                                    <span><%= rating %>/5</span>
                                                </div>
                                                <p><%= product.description %></p>
                                                <h6 class="my-4"><strong>Select Size:</strong></h6>
                                                <div class="sizes-container ">
                                                    <% if (product.size && Array.isArray(product.size)) { %>
                                                        <% product.size.forEach((size) => { %>
                                                            <div class="size-box">
                                                                <%= size %>
                                                            </div>
                                                        <% }) %>
                                                    <% } else { %>
                                                        <!-- Handle case where sizes are not available -->
                                                        <div>No sizes available</div>
                                                    <% } %>
                                                </div>
                                                <br>

                                                <!-- Product Price -->
                                                <h4><strong>Price: <%= product.price %></strong></h4>
                                                <!-- Add to Cart Button -->
                                                <button class="btn btn-primary mt-3">Add to Cart</button>
                                                <!-- Other Details -->
                                                <div class="other-details mt-4">
                                                    <table>
                                                        <tr>
                                                            <td>
                                                                <div class="me-3 my-3">
                                                                    <i class="fa fa-lock"></i>
                                                                    <span>Secure Payments</span>
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="mx-5 my-3">
                                                                    <i class="fa fa-ruler"></i>
                                                                    <span>Size and Fit</span>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <div class="me-3 my-3">

                                                                    <i class="fa fa-truck"></i>
                                                                    <span>Free Shipping</span>
                                                                </div>
                                                            </td>
                                                            <td>
                                                                <div class="mx-5 my-3">

                                                                    <i class="fa fa-undo"></i>
                                                                    <span>Free Shipping & Returns</span>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </div>
                                                
                                                 
                                                
                                            </div>
                                        </div>
                                    </div>
                                <% }) %>
                            </div>
                        </div>
                    <% }) %>
                <% } %>
            </div>
        </section>
        
